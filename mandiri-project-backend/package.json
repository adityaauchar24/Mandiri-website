{
  "name": "mandiri-backend",
  "version": "1.0.0",
  "description": "Backend API for PT. International Mandiri Expo - Local Development",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "local": "nodemon index.js",
    "server": "node index.js",
    "mongo:start": "docker run -d -p 27017:27017 --name mongodb-local mongo:latest",
    "mongo:stop": "docker stop mongodb-local && docker rm mongodb-local",
    "mongo:logs": "docker logs mongodb-local"
  },
  "keywords": [
    "express",
    "mongodb",
    "api",
    "backend",
    "local-development"
  ],
  "author": "PT. International Mandiri Expo",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}